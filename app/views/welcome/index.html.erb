<div class="jumbotron">
  <h1>Corkboard</h1>
  <p>Social Bookmarking with Email Integration</p>
  <p>
     <% if not current_user %>
      <%= link_to "Sign In", new_user_session_path, class: 'btn btn-primary' %> or 
      <%= link_to "Sign Up", new_user_registration_path, class: 'btn btn-primary' %>
     <% end %>
  </p>
</div>

<%= render partial: 'bookmarks/bookmarks', locals: { bookmarks: @bookmarks } %>
